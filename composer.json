{
    "name": "dawson/youtube",
    "description": "Upload a video to YouTube with Laravel 5.",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Dawson",
            "email": "hi@joedawson.me"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "google/apiclient": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Dawson\\Youtube\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Dawson\\Youtube\\YoutubeServiceProvider"
            ],
            "aliases": {
                "Youtube": "Dawson\\Youtube\\Facades\\Youtube"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
